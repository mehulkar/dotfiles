source $HOME/.env
source $HOME/.path
source $HOME/.aliases

if [ -f "$HOME/.secret" ]; then
  source "$HOME/.secret"
fi

# Docker shell setup
#eval "$(docker-machine env default)"

# Python shell setup
eval "$(pyenv init -)"
eval "$(pyenv virtualenv-init -)"

# Source RVM
[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"
